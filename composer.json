{
  "name": "plataformaregistro/regras",
  "description": "Regras implementadas para Plataforma Registro",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "slim/slim": "4.*",
    "slim/psr7": "^1.3",
    "doctrine/orm": "^2.6.2",
    "symfony/yaml": "2.*",
    "php-di/slim-bridge": "^3.0",
    "vlucas/phpdotenv": "^5.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "ext-libxml": "*",
    "ext-dom": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
